---
import { Image } from "astro:assets";
import icon from "@/assets/icon.jpg";
import ReactHeader from "./Header.tsx";
---

<ReactHeader client:load>
  <Image
    src={icon}
    alt="Icon"
    widths={[72]}
    sizes={"72px"}
    loading={"eager"}
    class:list={"rounded-full"}
  />
</ReactHeader>
